{
    "name": "odoo-client-ts",
    "version": "0.1.0",
    "description": "TypeScript client for Odoo JSON-RPC API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "vitest watch",
        "test": "vitest run",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "shell": "tsx scripts/repl.ts",
        "build": "tsc"
    },
    "keywords": [
        "odoo",
        "jsonrpc",
        "typescript",
        "client",
        "orm"
    ],
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "jsdom": "^26.0.0",
        "prettier": "^3.2.5",
        "tsx": "^4.19.3",
        "typescript": "^5.4.5",
        "vitest": "^1.4.0"
    },
    "dependencies": {
        "cross-fetch": "^4.1.0",
        "fetch-cookie": "^3.1.0",
        "tough-cookie": "^5.1.2"
    }
}
